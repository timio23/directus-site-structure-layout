{
	"name": "directus-site-structure-layout",
	"description": "Visulaizes a hierarchical table of webpages based on parent-child relationships, allowing users to intuitively explore and manage a site strcuture in Directus.",
	"icon": "extension",
	"author": "Tim Butterfield",
	"version": "0.3.0",
	"license": "MIT",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-layout",
		"site",
		"website",
		"hierarchy"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "layout",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^11.1.2"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "^14.0.0",
		"@directus/types": "^13.2.0",
		"@directus/utils": "^13.0.7",
		"@types/lodash": "^4.17.16",
		"@vueuse/core": "^13.1.0",
		"lodash": "^4.17.21",
		"typescript": "^5.8.3",
		"vue": "^3.5.17",
		"vue-i18n": "^11.1.7",
		"vue-router": "^4.5.1",
		"vuedraggable": "^2.24.3"
	}
}
